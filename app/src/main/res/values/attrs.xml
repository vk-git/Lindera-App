<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolBar">
        <attr name="android:text"/>
        <attr name="android:textColor"/>
        <attr name="android:textSize"/>
        <attr name="leftDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="AutoViewPager">
        <attr format="boolean" name="avp_indeterminate"/>
        <attr format="boolean" name="avp_autoScroll"/>
        <attr format="integer" name="avp_duration"/>
    </declare-styleable>

</resources>